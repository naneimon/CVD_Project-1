# Create a boxplot with median, Q1, Q3, and colored outliers
p <- ggplot(data, aes(x = x, y = y)) +
geom_boxplot(outlier.colour = "red", outlier.shape = 1) +
geom_point(
data = data %>%
group_by(x) %>%
summarize(Median = median(y), Q1 = quantile(y, 0.25), Q3 = quantile(y, 0.75)),
aes(x = x, y = Median),
color = "blue",
size = 3
) +
